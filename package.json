    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "jetifier": "^1.6.3",
    "metro-react-native-babel-preset": "^0.55.0",
    "prettier": "1.18.2",
    "react-test-renderer": "^16.8.6"
  },
  "prettier": {
    "semi": false,
