    "@react-native-firebase/database": "^6.0.0-alpha.25",
    "humanize-duration": "^3.20.0",
    "i18n-js": "^3.3.0",
    "jsc-android": "^241213.1.0",
    "lodash": "^4.17.15",
    "mobx": "^5.13.0",
    "mobx-persist": "^0.4.1",
    "mobx-react": "5.4.4",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-native": "^0.60.4",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-google-signin": "^2.0.0",
    "react-native-localize": "^1.1.4",
